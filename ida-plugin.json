{
  "IDAMetadataDescriptorVersion": 1,
  "plugin": {
    "name": "HappyIDA",
    "description" : "Make your IDA Happy!",
    "authors": [
      {
        "name": "HappyIDA Team",
        "email": ""
      },
      {
        "name": "NiNi",
        "email": "nini@undefined.zip"
      },
      {
        "name": "HexRabbit",
        "email": "h3xrabbit@gmail.com"
      },
      {
        "name": "Angelboy",
        "email": "scwuaptx@gmail.com"
      }
    ],
    "license": "GPL-3.0",
    "version": "1.0.0",
    "entryPoint": "main.py",
    "idaVersions": ">=9.0",
    "categories": ["decompilation"],
    "keywords": [
      "decompiler",
      "parameter label",
      "SEH"
    ],
    "urls": {
      "repository": "https://github.com/HappyIDA/HappyIDA"
    },
    "pythonDependencies": [
      "ida-settings==3.3.0"
    ],
    "settings": [
      {
        "key": "seh_bgcolor",
        "type": "string",
        "default": "8FE0F8",
        "name": "SEH Highlight Background Color",
        "documentation": "The background color of the SEH highlight. Format: RRGGBB",
        "validation_pattern": "[0-9A-Fa-f]{6}",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_param_label",
        "type": "boolean",
        "default": true,
        "name": "Enable Parameter Labeling",
        "documentation": "Enable parameter labeling",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_param_edit",
        "type": "boolean",
        "default": true,
        "name": "Enable Parameter Edit",
        "documentation": "Enable parameter edit",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_param_sync_name",
        "type": "boolean",
        "default": true,
        "name": "Enable Parameter Sync Name",
        "documentation": "Enable parameter sync name",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_param_sync_type",
        "type": "boolean",
        "default": true,
        "name": "Enable Parameter Sync Type",
        "documentation": "Enable parameter sync type",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_func_navigate",
        "type": "boolean",
        "default": true,
        "name": "Enable Function Navigate",
        "documentation": "Enable function navigate",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_rust_string",
        "type": "boolean",
        "default": true,
        "name": "Enable Rust String",
        "documentation": "Enable rust string",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_seh_highlight",
        "type": "boolean",
        "default": true,
        "name": "Enable SEH Highlight",
        "documentation": "Enable seh highlight",
        "prompt": false,
        "required": true
      },
      {
        "key": "enable_seh_rebuild",
        "type": "boolean",
        "default": true,
        "name": "Enable SEH Rebuild",
        "documentation": "Enable seh rebuild",
        "prompt": false,
        "required": true
      }
    ]
  }
}
